    <script>

        function handleSort() {
            const sortBy = document.getElementById('sortBy').value;
            const urlParams = new URLSearchParams(window.location.search);
            urlParams.set('sort', sortBy);
            const newUrl = `${window.location.pathname}?${urlParams.toString()}`;
            window.location.href = newUrl;
        }

        function applySearch() {
            const searchQuery = document.getElementById('searchInput').value;
            const urlParams = new URLSearchParams(window.location.search);
            urlParams.set('search', searchQuery);
            urlParams.delete('page'); 
            window.location.href = `${window.location.pathname}?${urlParams.toString()}`;
        }

        window.onload = function () {
            const urlParams = new URLSearchParams(window.location.search);
            const selectedSort = urlParams.get('sort') || 'latest';
            document.getElementById('sortBy').value = selectedSort;
        }

        function applyFilter(filterType, filterValue) {
            const urlParams = new URLSearchParams(window.location.search);

            if (filterType === 'category') {
                if (filterValue) {
                    urlParams.set('category', filterValue);
                } else {
                    urlParams.delete('category');
                }
                urlParams.delete('brand'); 
            } else if (filterType === 'brand') {
                if (filterValue) {
                    urlParams.set('brand', filterValue);
                } else {
                    urlParams.delete('brand'); 
                }
                urlParams.delete('category'); 
            }

            urlParams.set('page', 1);
            const newUrl = `${window.location.pathname}?${urlParams.toString()}`;
            window.location.href = newUrl;
        }

        function applyOutOfStockFilter() {
            const urlParams = new URLSearchParams(window.location.search);
            const outOfStockChecked = document.getElementById("outOfStockCheckbox").checked;

            if (outOfStockChecked) {
                urlParams.set('outOfStock', 'true');
            } else {
                urlParams.delete('outOfStock');
            }

            urlParams.set('page', 1);
            window.location.href = `${window.location.pathname}?${urlParams.toString()}`;
        }

        window.onload = function () {
            const urlParams = new URLSearchParams(window.location.search);
            const selectedSort = urlParams.get('sort') || 'latest';
            document.getElementById('sortBy').value = selectedSort;

            if (urlParams.get('outOfStock') === 'true') {
                document.getElementById("outOfStockCheckbox").checked = true;
            }
        };



    </script>




